{
  "presets": [
    [
      "@babel/preset-env",
      { "targets": { "browsers": ["last 2 versions", ">= 5% in KR"] } }
    ],
    ["@babel/preset-react", { "runtime": "automatic" }],
    "@babel/typescript"
  ],
  "plugins": [
    ["module-resolver", { "root": ["./src"] }],
    "babel-plugin-styled-components"
  ]
}
